#include <iostream>
#include <set>

using namespace std;

int n;
char vowels[6] = {"aeiou"};
char consonants[21] = { "bcdfghjklmnpqrstvwxz"};

void drill(int n, int vowel){
	if(n == 0){
		cout << endl;
		return;
	}else{
		if(vowel){
			for(int i = 0; i < 6; i++){
				cout << vowels[i];
				drill(n-1, false);
			}
		}else{
			for(int i = 0; i < 19; i++){
				cout << consonants[i];
				drill(n-1, true);
			}
		}
	}
}

int main() {
	cin >> n;
	drill(n, true);
	drill(n, false);
	return 0;
}
