#include <iostream>
#include <string>

using namespace std;

int n, m;
char vowels[6] = {"aeiou"};
char consonants[21] = { "bcdfghjklmnpqrstvwxz"};
string cur = "";


void drill(int n, int vowel){
	if(n == 0){
		cout << endl;
		return;
	}else{
		if(vowel){
			for(int i = 0; i < 5; i++){
				cur.push_back(vowel[i]);
				drill(n-1, 1-vowel);
				cur.pop_back(vowel[i]);
			}
		}else{
			for(int i = 0; i < 20; i++){
				cur.push_back(consonant[i]);
				drill(cur, n-1, true);
				cur.pop_back(vowel[i]);
			}
		}
	}
}

int main() {
	cin >> n;
	m = n;
	drill(cur, n, true);
	drill(cur, n, false);
	return 0;
}
