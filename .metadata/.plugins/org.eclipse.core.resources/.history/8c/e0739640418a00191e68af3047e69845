#include <iostream>

using namespace std;

int n, m;
char vowels[6] = {"aeiou"};
char consonants[21] = { "bcdfghjklmnpqrstvwxz"};
char cur[7];

inline int drill(char cur[7], int n, int vowel){
	if(n == 0){
		cout << endl;
		return 1;
	}else{
		if(vowel){
			for(int i = 0; i < 5; i++){
				while(true){
					cout << vowels[i];
					if(drill(cur, n-1, false)){
						break;
					}
			}
		}else{
			for(int i = 0; i < 20; i++){
				while(true){
					cout << consonants[i];
					drill(cur, n-1, true);
				}
			}
		}
	}
}

int main() {
	cin >> n;
	m = n;
	drill(cur, n, true);
	drill(cur, n, false);
	return 0;
}
